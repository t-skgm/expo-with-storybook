{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "sourceMap": true,
    "experimentalDecorators": true,
    "jsx": "react-native",
    "lib": ["dom", "es2015", "es2016", "esnext.asynciterable"],
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "strict": true,
    "noImplicitReturns": true,
    "baseUrl": ".",
    "paths": {
      "@components*": ["src/components/*"]
    }
  }
}
